    "version": "0.2.0",
    "configurations": [
        {
            "name": "c_algorithms_challenge debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/c_algorithms_challenge",
            "args": ["-v"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "cpp_algorithms_challenge debug",
